    "import matplotlib.pyplot as plt\n",
    "\n",
    "harsh_only = results_df[results_df['Severity'] == 'Harsh'].copy()\n",
    "harsh_only['Difference'] = harsh_only['Male'] - harsh_only['Female']\n",
    "\n",
    "# Count how many crimes support hypothesis\n",
    "positive_diff = (harsh_only['Difference'] > 0).sum()\n",
    "negative_diff = (harsh_only['Difference'] < 0).sum()\n",
    "total_crimes = len(harsh_only)\n",
    "\n",
    "# Top 10 crimes where females get lighter punishment\n",
    "top_10 = harsh_only.nlargest(10, 'Difference')[['Crime', 'Male', 'Female', 'Difference']]\n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(14, 6))\n",
    "\n",
    "# Left: Summary bar chart\n",
    "axes[0].bar(['Males Punished\\nHarshly More', 'Females Punished\\nHarshly More'], \n",
    "            [positive_diff, negative_diff], color=['steelblue', 'coral'])\n",
    "axes[0].set_ylabel('Number of Crime Types')\n",
    "axes[0].set_title(f'Gender Bias in Harsh Punishments\\n({positive_diff}/{total_crimes} crimes favor females)')\n",
    "axes[0].set_ylim(0, total_crimes)\n",
    "\n",
    "# Right: Top 10 crimes with biggest gaps\n",
    "axes[1].barh(range(len(top_10)), top_10['Difference'], color='steelblue')\n",
    "axes[1].set_yticks(range(len(top_10)))\n",
    "axes[1].set_yticklabels(top_10['Crime'])\n",
    "axes[1].set_xlabel('Difference in Harsh Punishment Rate\\n(Male % - Female %)')\n",
    "axes[1].set_title('Top 10 Crimes with Largest Gender Gap')\n",
    "axes[1].invert_yaxis()\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\nSummary: In {positive_diff} out of {total_crimes} crime types ({positive_diff/total_crimes*100:.1f}%), \"\n",
    "      f\"males received harsh punishments at higher rates than females.\")"
